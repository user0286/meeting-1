var myData = {
  name:"malabulu",
  age:1083,
  maiden:0,

};

console.log(myData.name)
console.log(myData.age)
console.log(myData.maiden)

//test object block

var block = {
  length:10,
  width:5,
  height:4,
};


function volume(){
  var vol = block.length*block.width*block.height
  return "the volume of the block is : " + vol
}

console.log(volume());

//test object block with method 

var block = {
  length:10,
  width:5,
  height:4,
  volume: function(){
    return this.length*this.width*this.height
      
  }
};

console.log(block.volume())


